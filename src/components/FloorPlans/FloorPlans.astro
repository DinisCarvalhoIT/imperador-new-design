---
import { Image } from "astro:assets";
import floorPlansImage from "../../../public/FloorPlans/Rectangle 34624212.png";
import FloorPlansCarousel from "./FloorPlansCarousel";
---

<section class="w-full relative min-h-screen flex items-center justify-center">
  <div class="inset-0 w-full h-full">
    <Image
      src={floorPlansImage}
      alt="Floor Plans"
      class="w-full h-full object-cover"
    />
  </div>

  <FloorPlansCarousel client:load />
</section>
