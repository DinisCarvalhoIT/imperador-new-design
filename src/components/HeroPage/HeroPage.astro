---
import { Image } from "astro:assets";
import heroImg from "../../../public/HomePage/homePageImage.png";
---

<section class="w-full h-screen relative">
  <Image
    src={heroImg}
    alt="Home hero image"   
    class="w-full h-full object-cover"
    widths={[640, 960, 1200, 1920, 2560]}
    sizes="100vw"
    loading="eager"
    decoding="sync"
    fetchpriority="high"
  />
</section>
