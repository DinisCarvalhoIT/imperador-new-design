---
import { Image } from "astro:assets";
import floorPlansImage from "../../../public/FloorPlans/Rectangle 34624212.png";
---

<section class="w-full min-h-screen relative">
  <div class="w-full h-full">
    <Image
      src={floorPlansImage}
      alt="Floor Plans"
      class="w-full h-full object-cover"
    />
  </div>

  <!-- Card overlay -->
  <div class="absolute inset-0 flex items-center justify-center p-6">
    <div
      class="bg-white/86 border border-[rgba(74,125,145,0.11)] rounded-2xl p-8 max-w-[80%] w-full shadow-lg"
    >
    </div>
  </div>
</section>
